<% 
    function printOK(){
        console.log("OK");
    }
%>
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>api test</title>
</head>
<style>
    body{
        background-color: rgb(15, 15, 15);
        color: rgb(252, 249, 247);
    }
    table, tr, td{
        border-collapse: collapse;
        border: rgb(150,150,150) 1px solid;
    }
    td{
        padding: 5px;
    }
    .api-fetcher{
        background-color: rgb(41, 64, 0);
    }
</style>
<body>
    <table>
        <tr>
            <td>name</td> 
            <td>value</td> 
        </tr>
        <% for( let i = 0; i < stats.length; i++ ) {printOK() %>
            <tr>
                <td><%= stats[i].name %></td> 
                <td 
                    port="<%= stats[i].port %>" 
                    unit="<%= stats[i].unit %>" 
                    resource="<%= stats[i].resource %>" 
                    class="api-fetcher"
                    component="<%= stats[i].component %>"
                >
                </td> 
            </tr>
        <% } %>
    </table>
    

    <script src="JS/apiUpdateWorker.js"></script>
</body>
</html>